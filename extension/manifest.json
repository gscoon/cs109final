{
    "name": "Expert",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Who really knows this stuff?",
    "homepage_url": "http://cs109.github.io/2015/",
    "icons": {
        "16": "images/icons/icon16.png",
        "38": "images/icons/icon38.png",
        "48": "images/icons/icon48.png",
        "128": "images/icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "js/jquery.js",
            "js/background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icons/icon19.png",
            "38": "images/icons/icon38.png"
        },
        "default_title": "Expert"
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "css/inject.css"
            ]
        },
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/jquery.js",
                "js/inject.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "images/*"
    ]
}
